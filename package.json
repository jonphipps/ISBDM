{
  "name": "isbdm-html-to-mdx",
  "version": "1.0.0",
  "description": "HTML to MDX converter for IFLA ISBDM documentation",
  "main": "html-to-mdx.js",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "start:patched": "node start-patched.js",
    "build": "docusaurus build",
    "build:patched": "node build-patched.js",
    "generate-redirects": "node scripts/generate-element-redirects.js",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "typecheck": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:mdx": "remark . --ext .mdx,.md",
    "lint:eslint": "eslint .",
    "format:mdx": "remark --rc-path .remarkrc.js \"docs/**/*.{md,mdx}\" --output",
    "dev": "node scripts/dev.js",
    "list": "node scripts/dev.js list",
    "generate": "node scripts/dev.js generate",
    "parse-prd": "node scripts/dev.js parse-prd",
    "dctap:generate": "node scripts/dctap-generate.js --profile ./profiles/current.csv --out ./spreadsheets/template.xlsx",
    "sheets:ingest": "node scripts/sheets-to-mdx.js --sheet 'https://docs.google.com/...'",
    "docs:build": "docusaurus build",
    "vocab:release": "node scripts/release-vocab.js",
    "docs:release": "node scripts/release-docs.js",
    "ci:test": "yarn lint && yarn run test",
    "ci:build-all": "yarn docs:build && yarn vocab:release --dry-run",
    "sheets:import": "ts-node scripts/sheets-import.ts",
    "sheets:export": "ts-node scripts/sheets-export.ts",
    "sheet:create": "tsx scripts/create-sheet.ts",
    "vocabulary:create": "tsx scripts/create-vocabulary-sheet.ts",
    "vocabulary:create-action": "tsx scripts/create-vocabulary-sheet-action.ts",
    "detect:language-mismatches": "node scripts/detect-language-mismatches.js",
    "detect:language-mismatches-local": "node scripts/detect-language-mismatches-local.js",
    "detect:language-mismatches-skos": "node scripts/detect-language-mismatches-skos.js",
    "check:language-tags": "node scripts/check-mediatype-languages.js",
    "check:language-tags:md": "node scripts/check-mediatype-languages.js --markdown",
    "check:language-tags:ai": "node scripts/check-mediatype-languages.js --ai",
    "check:language-tags:ai:md": "node scripts/check-mediatype-languages.js --ai --markdown",
    "check:language-tags:ai:test": "node scripts/check-mediatype-languages.js --ai --test",
    "check:language-tags:help": "node scripts/check-mediatype-languages.js --help"
  },
